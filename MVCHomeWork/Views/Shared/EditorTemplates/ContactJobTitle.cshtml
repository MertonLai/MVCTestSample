@model string
@{ 
    string _TmpData = "";
    if (!string.IsNullOrWhiteSpace(Model)) {
        _TmpData = Model;
    }
    IEnumerable<SelectListItem> JobTitleList = new MVCHomeWork.Controllers.TemplateDataController().GetJobTitleList(_TmpData);
}
@Html.DropDownList("", JobTitleList, new { @id = "JobTitleList" })